<?php

namespace Module\{%uModuleName%}\Entity;

use JsonSerializable;
use Doctrine\ORM\Mapping as ORM;
use Module\{%uModuleName%}\Repository\{%repositoryName%};

#[ORM\Entity(repositoryClass: {%repositoryName%}::class)]
#[ORM\Table(name: '{%lModuleName%}')]
class {%entityName%} implements JsonSerializable
{
    #[ORM\Id]
    #[ORM\GeneratedValue]
    #[ORM\Column(type: 'integer')]
    private int $id;

    /**
     * @return int
     */
    public function getId(): int
    {
        return $this->id;
    }

    /**
     * @param int $id
     */
    public function setId(int $id): void
    {
        $this->id = $id;
    }

    public function jsonSerialize(): array
    {
        return [
            'id' => $this->getId(),
            'name' => $this->getName()
        ];
    }
}